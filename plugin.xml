<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension
         point="org.eclipse.ui.commands">
      <command
            defaultHandler="edu.ucsc.codevo.controller.AnalyzingHandler"
            id="Codevo.analyze"
            name="Analyze">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.handlers">
   </extension>
   <extension
         point="org.eclipse.ui.bindings">
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            allPopups="false"
            locationURI="menu:org.eclipse.ui.main.menu">
         <menu
               id="codevo"
               label="Codevo">
            <command
                  commandId="Codevo.analyze"
                  label="Analyze"
                  style="push">
            </command>
         </menu>
      </menuContribution>
   </extension>
   <extension
         point="org.eclipse.ui.views">
      <category
            id="Codevo.defaultCategory"
            name="Codevo">
      </category>
      <view
            category="Codevo.defaultCategory"
            class="edu.ucsc.codevo.view.DependencyView"
            id="Codevo.dependencyView"
            name="Dependency View"
            restorable="true">
      </view>
   </extension>

</plugin>
